{
  "name": "algolia_parcel",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "parcel ./src/index.html --port 3001",
    "build": "parcel build ./src/index.html --no-source-maps"
  },
  "devDependencies": {
    "parcel": "^2.9.2",
    "parcel-namer-relative-to-cwd": "^0.0.0-alpha.0",
    "parcel-namer-rewrite": "^2.0.0-rc.3",
    "postcss": "^8.4.16",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {},
    "parcel-namer-rewrite": {
      "hashing": "never",
      "rules": {
        "styles/(.*).css": "$1.css",
        "js/(.*).js": "$1.js"
      },
      "profiles": {
        "development": {
          "disable": true
        }
      }
    }
}

{
  "name": "algolia_parcel",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development && parcel ./src/index.html --port 3001  --public-url=/ --host localhost",
    "build": "NODE_ENV=production && npm run clean && parcel build ./src/index.html --no-source-maps",
    "clean": "rm -rf .parcel-cache && rm -rf dist"
  },
  "devDependencies": {
    "parcel": "^2.9.2",
    "parcel-namer-rewrite": "^2.0.0-rc.3",
    "parcel-plugin-x": "./local_modules/parcel-plugin-x",
    "postcss": "^8.4.16",
    "postcss-nested": "^5.0.6",
    "tailwindcss": "^3.1.8"
  },
  "dependencies": {},
  "parcel-namer-rewrite": {
    "hashing": "never",
    "rules": {
      "index(.*).js": "algolia-search-bar.js",
      "search(.*).js": "algolia-search-results.js",
      "index(.*).css":"algolia-styles.css"
    }
  }
}
